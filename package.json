{
    "name": "vscode-cmake-support",
    "version": "0.1.18",
    "publisher": "mycoshiro",
    "displayName": "CMake Support [FORK]",
    "description": "Additional CMake support for Visual Studio Code",
    "license": "MIT",
    "icon": "data/images/icon.png",
    "main": "./out/extension",
    "packageManager": "npm@10.5.2",
    "engines": {
        "vscode": "^1.74.0",
        "node": ">=18",
        "npm": ">=9"
    },
    "author": {
        "name": "Nicolas Martin"
    },
    "maintainers": [
        "Joel Van Eenwyk"
    ],
    "scripts": {
        "test": "tsc -p ./ && vscode-test",
        "test:mocha": "mocha",
        "test:vscode": "npm run compile && vscode-test",
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "build": "npm run compile && vsce package --no-dependencies",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./",
        "deploy": "vsce publish --no-dependencies",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/joelvaneenwyk/vs.language.cmake"
    },
    "bugs": "https://github.com/joelvaneenwyk/vs.language.cmake/issues",
    "homepage": "https://github.com/joelvaneenwyk/vs.language.cmake/blob/develop/README.md",
    "keywords": [
        "CMake",
        "Syntax"
    ],
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "resolutions": {
        "glob": ">=10.3.12"
    },
    "peerDependencies": {
        "eslint": ">=6.0.0",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0"
    },
    "dependencies": {
        "@types/eslint": "^8.56.10",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.12.7",
        "@types/opener": "^1.4.3",
        "@types/vscode": "^1.88.0",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.3.9",
        "@vscode/vsce": "^2.26.0",
        "eslint-config-love": "^47.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-functional": "^6.5.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsdoc": "^48.2.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-prettier-config": "^1.0.1",
        "mocha": "^10.4.0",
        "opener": "^1.5.2",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "vsce": {
        "dependencies": false,
        "yarn": false
    },
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "activationEvents": [
        "onLanguage:cmake"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cmake",
                "extensions": [
                    ".cmake"
                ],
                "filenames": [
                    "CMakelists.txt"
                ],
                "aliases": [
                    "CMake"
                ]
            },
            {
                "id": "cmake-cache",
                "filenames": [
                    "CMakeCache.txt"
                ],
                "aliases": [
                    "CMake Cache"
                ]
            }
        ],
        "commands": [
            {
                "command": "cmake.onlineHelp",
                "title": "CMake: Online Help"
            }
        ],
        "grammars": [
            {
                "language": "cmake",
                "scopeName": "source.cmake",
                "path": "./syntaxes/CMake.tmLanguage"
            },
            {
                "language": "cmake-cache",
                "scopeName": "source.cmakecache",
                "path": "./syntaxes/CMakeCache.tmLanguage"
            }
        ],
        "configuration": {
            "title": "CMake configuration",
            "properties": {
                "cmake.cmakePath": {
                    "type": "string",
                    "default": "cmake",
                    "description": "The path to CMake generator executable"
                }
            }
        }
    }
}
